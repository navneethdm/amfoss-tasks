#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    int T,X,Y;
    scanf("%d",&T);

    if (T>=1 && T<=1000)
    {
        while (T--) 
        {
            scanf("%d %d",&X,&Y);
            if (X>Y)
            {
                printf("%d\n",(X-1)/10 - (Y-1)/10);
            }
            else if (Y>X)
            {
                printf("%d\n",(Y-1)/10 - (X-1)/10);
            }
            else if (X==Y)
                return 0;
        }
    }
    return 0;
}
